<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record model="ir.ui.view" id="view_delivery_master_form">
        <field name="name">delivery.master.form</field>
        <field name="model">delivery.master</field>
        <field name="arch" type="xml">
            <form string="Delivery Master">
                <group>
                    <field name="carrier_code" />
                    <field name="name" />
                    <field name="site_url" />
                </group>
            </form>
        </field>
    </record>
    <record model="ir.ui.view" id="view_delivery_master_tree">
        <field name="name">delivery.master.tree</field>
        <field name="model">delivery.master</field>
        <field name="arch" type="xml">
            <tree
                string="Delivery Master"
                create="false"
                edit="false"
            >
                <field name="carrier_code" />
                <field name="name" />
                <field name="site_url" />
            </tree>
        </field>
    </record>
    <record
        id="delivery_master_act"
        model="ir.actions.act_window"
    >
        <field name="name">Delivery Master</field>
        <field name="res_model">delivery.master</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form</field>
        <field name="view_id" ref="view_delivery_master_form" />
        <field name="target">new</field>
        <field name="context">{}</field>
    </record>
    <menuitem
        id="stock_delivery_result_import_wizard_menu"
        name="Delivery Master"
        parent="stock.menu_stock_config_settings"
        groups="stock.group_stock_manager"
        action="delivery_master_act"
        sequence="30"
    />
</odoo>
